{
    "name": "yaraku/text-diff",
    "description": "Text diff generation and rendering library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Text_Diff",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        },
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        }
    ],
    "version": "2.3.0",
    "time": "2021-06-11",
    "require": {
        "php": "^8",
        "ext-libxml": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "3.4.*"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Text_Diff": "2.*",
        "pear-horde/Horde_Text_Diff": "2.*",
        "horde/text-diff": "2.*"
    },
    "provide": {
        "horde/text-diff": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Horde\\": "lib/Horde/"
        }
    }
}
